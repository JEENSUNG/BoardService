{{>layout/header}}
<div id="posts_list">
    <div class="container col-md-6">
        <form>
            <div class="form-group">
                <label>비밀번호</label>
                <input type="password" class="form-control" id="password1" name="password1" placeholder="비밀번호를 입력해주세요">
            </div>

            <div class="form-group">
                <label>비밀번호 재확인</label>
                <input type="password" class="form-control" id="password2" name="password2" placeholder="비밀번호를 한 번 더 입력해주세요">
            </div>
            <button id = "btn-user-password-modify" class="form-control btn btn-secondary bi bi-lock-fill">비밀번호 변경</button>
        </form>
    </div>
</div>
